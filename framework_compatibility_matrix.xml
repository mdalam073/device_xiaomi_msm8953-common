<compatibility-matrix version="2.0" type="framework">
    <!-- Biometrics Fingerprint HAL -->
    <hal format="hidl">
        <name>android.hardware.biometrics.fingerprint</name>
        <version>2.3</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IBiometricsFingerprint</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Bluetooth Audio Provider HAL -->
    <hal format="aidl">
        <name>android.hardware.bluetooth.audio</name>
        <version>2.0</version> <!-- Adjust based on correct version -->
        <transport>hwbinder</transport>
        <interface>
            <name>IBluetoothAudioProviderFactory</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Media CAS HAL -->
    <hal format="hidl">
        <name>android.hardware.cas</name>
        <version>1.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IMediaCasService</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Drm HAL -->
    <hal format="hidl">
        <name>android.hardware.drm</name>
        <version>1.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IDrmFactory</name>
            <instance>clearkey</instance>
        </interface>
    </hal>

    <!-- Health HAL -->
    <hal format="hidl">
        <name>android.hardware.health</name>
        <version>3.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IHealth</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Lights HAL -->
    <hal format="hidl">
        <name>android.hardware.light</name>
        <version>2.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>ILights</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Power HAL -->
    <hal format="hidl">
        <name>android.hardware.power</name>
        <version>3.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IPower</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- USB HAL -->
    <hal format="hidl">
        <name>android.hardware.usb</name>
        <version>1.3</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IUsb</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Vibrator HAL -->
    <hal format="hidl">
        <name>android.hardware.vibrator</name>
        <version>1.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IVibrator</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Wifi HAL -->
    <hal format="hidl">
        <name>android.hardware.wifi</name>
        <version>2.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IWifi</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Wifi Hostapd HAL -->
    <hal format="hidl">
        <name>android.hardware.wifi.hostapd</name>
        <version>2.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>IHostapd</name>
            <instance>default</instance>
        </interface>
    </hal>

    <!-- Wifi Supplicant HAL -->
    <hal format="hidl">
        <name>android.hardware.wifi.supplicant</name>
        <version>3.0</version>
        <transport>hwbinder</transport>
        <interface>
            <name>ISupplicant</name>
            <instance>default</instance>
        </interface>
    </hal>

   <hal format="hidl">
        <name>vendor.qti.hardware.radio.atcmdfwd</name>
        <version>1.0</version>
        <interface>
            <name>IAtCmdFwd</name>
            <instance>AtCmdFwdService</instance>
        </interface>
    </hal>
</compatibility-matrix>
